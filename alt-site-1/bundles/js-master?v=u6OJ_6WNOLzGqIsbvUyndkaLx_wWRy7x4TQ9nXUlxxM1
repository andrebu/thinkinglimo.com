function responsive(n){baseFont(n)}function appInit(){$(".carousel").size()>0&&$(".carousel").each(function(){createCarousel($(this))});$("nav a, .nav a").off("click.goToSection").on("click.goToSection",function(){setTimeout(function(){hashSlideEnable()},100)});if(customForm(),$(".cityDropdown").size()>0)$(".cityDropdown .dropdown-menu a").off("click.changeCity").on("click.changeCity",function(n){var t,u,o,f;n.preventDefault();var r=$(this),s=$(".drivrDropdown .dropdown-menu li.active a"),e=s.text();e=e.slice(0,e.indexOf("-")-1),r.parent("li").addClass("active").siblings().removeClass("active"),r.parents(".cityDropdown").first().find("span.text").text(r.text()),t={},t.text=r.data("text"),t.rate=[],t.rate=(r.data("fare")+"").replace(/[^\d.,]/g,"").split(","),t.pricing={},r.siblings("input").each(function(){$(this).data("rel")=="drivr"?t.pricing.drivr=$(this).data("fares").replace(/[^\d.,]/g,"").split(","):$(this).data("rel")=="overdrivr"&&(t.pricing.over=$(this).data("fares").replace(/[^\d.,]/g,"").split(","))}),u=$(".cityDropdown").parent().find(".table1"),o=$(".cityDropdown").parent().find(".table2"),u.find("span.text").html(t.text),u.find("span.rate-drivr").text(t.rate[0]),u.find("span.rate-over").text(t.rate[1]);for(i in t.pricing.drivr)f=parseInt(i,10)+1,o.find("span.drivr.rate"+f).text(t.pricing.drivr[i]);for(i in t.pricing.over)f=parseInt(i,10)+1,o.find("span.overdrivr.rate"+f).text(t.pricing.over[i])});if($("#notify").size()>0)$("#notify").off("submit.notify").on("submit.notify",function(n){n.preventDefault();var t=$("#email"),i=$(this);t.val()&&validEmail(t.val())?$.ajax({url:i.attr("action"),type:"POST",cache:!1,data:{email:t.val()},success:function(n){t.val("");var u="You email has been submitted successfully and you will be contacted once we have news about the app!",f=parseInt(n,10);n==-1?u="No email address submitted!":n==1&&(u="We have encountered an error. Please try again later!"),t.popover("destroy"),clearTimeout(timeout),t.popover({placement:"top",trigger:"manual",content:u}),t.popover("show"),hidePopOver(t)},error:function(){t.popover("destroy"),clearTimeout(timeout),t.popover({placement:"top",trigger:"manual",content:"We have encountered an error. Please try again later!"}),t.popover("show"),hidePopOver(t)}}):(t.popover("destroy"),clearTimeout(timeout),t.popover({placement:"top",trigger:"manual",content:"Invalid email address. Please write in your email address!"}),t.popover("show"),hidePopOver(t))});if($(".stand-alone-image").size()>0){var n=$(".stand-alone-image").first();$(window).off("scroll.imageScroll").on("scroll.imageScroll",function(){var t=$(window).scrollTop()/2,i;$("header").hasClass("_signinvisible")&&(i=getRealHeight($("section.signinform")),t=i>$(window).scrollTop()?0:($(window).scrollTop()-i)/2),n.css("background-position","0 "+t+"px"),n.parent().css("background-position","0 "+t+"px")})}if($("._countrySelect").size()>0&&countrySelector(),$("a.signin").size()>0){$("a.signin").off("click.toggleSignin").on("click.toggleSignin",function(n){if(n.preventDefault(),!$("section.signinform").hasClass("_animating")&&!$("section.signinform").hasClass("_showing")){$("section.signinform").addClass("_animating"),$("body, html").scrollTop(0),$("section.signinform").show();var t=getRealHeight($("section.signinform"));$("section.signinform").removeAttr("style"),$("section.signinform").slideDown(300,function(){$("section.signinform").removeClass("_animating").addClass("_showing"),$("header").addClass("_signinvisible")}),$("header").animate({top:t+"px"},300)}});$("a.closesignin").off("click.toggleSignin").on("click.toggleSignin",function(n){n.preventDefault(),!$("section.signinform").hasClass("_animating")&&$("section.signinform").hasClass("_showing")&&($("section.signinform").addClass("_animating"),$("section.signinform").slideUp(300,function(){$("section.signinform").removeClass("_animating _showing"),$("header").removeClass("_signinvisible")}),$("header").animate({top:"0px"},300))})}}function hidePopOver(n){timeout=setTimeout(function(){n.popover("destroy")},5e3)}function countrySelector(){$("._citySelect a").off("click.selectCity").on("click.selectCity",function(n){n.preventDefault();var t=$(this).text();$("._citySelect").siblings("a").first().html(t+'<span class="caret"><\/span>')});$("._countrySelect a").off("click.selectCountry").on("click.selectCountry",function(n){var t,u,r;n.preventDefault(),t=$(this).siblings("input[type='hidden']").first().val().split(","),u=$(this).text(),$("._countrySelect").siblings("a").first().html(u+'<span class="caret"><\/span>'),r="";for(i in t)r+='<li><a href="javascript:void(0);">'+t[i]+"<\/a><\/li>";$("._citySelect").html(r);$("._citySelect a").off("click.selectCity").on("click.selectCity",function(n){n.preventDefault();var t=$(this).text();$("._citySelect").siblings("a").first().html(t+'<span class="caret"><\/span>')});$("._citySelect").siblings("a").first().html(t[0]+'<span class="caret"><\/span>')})}function createCarousel(n){n.carousel({interval:parseInt(n.data("interval"),10)});n.find(".rightButtonHolder").off("click.slide").on("click.slide",function(){n.carousel("next")});if(!uad.isDesktop){n.off("touchstart.swipe").on("touchstart.swipe",function(n){n.preventDefault(),newEv=!0,startSwipe=n.originalEvent.touches?n.originalEvent.touches[0].pageX:n.changedTouches[0].pageX});n.off("touchmove.swipe").on("touchmove.swipe",function(t){if(t.preventDefault(),endSwipe=t.originalEvent.touches?t.originalEvent.touches[0].pageX:t.changedTouches[0].pageX,startSwipe===!1||endSwipe===!1)return!1;var i=startSwipe-endSwipe;Math.abs(i)>=100&&newEv&&(newEv=!1,i>0?n.carousel("next"):n.carousel("prev"))})}if(n.hasClass("topCarousel"))n.off("slid.endSlide").on("slid.endSlide",function(){n.find(".carousel-status").size()>0&&n.find(".carousel-status").children("span").text(n.find(".item.active").index()+1+"/"+n.find(".item").size());var t=n.find(".rightButtonHolder").children("p").first(),i=n.find(".item.active").data("caption");t.fadeOut(200,function(){t.html(i).fadeIn(200)})})}function disableNativeAnchors(){$("section").each(function(){$(this).data("id",$(this).attr("id")).removeAttr("id")})}function hashSlideEnable(){if(window.location.hash){var n=window.location.hash.substring(1);$("section").each(function(){$(this).data("id")==n&&slideToHash($(this))}),$("header nav a").each(function(){$(this).attr("href").indexOf("#"+n)!=-1&&($("header nav a").removeClass("active"),$(this).addClass("active"))})}}function slideToHash(n){var t=n.offset().top-getRealHeight($("header"));uad.isDesktop||$(window).off("scroll.winScroll"),$("html, body").animate({scrollTop:t+"px"},"slow",function(){var n,t;uad.isDesktop||($("header._absolute").size()>0&&(n=$("header._absolute"),n.removeClass("_transition"),t=$(window).scrollTop()-getRealHeight(n)-25,n.css("top",t+"px"),n.show(),n.addClass("_transition"),n.css("top",$(window).scrollTop()+"px")),windowScroll())})}function customForm(){$("input.customInput").size()>0&&customCheckbox(),$("input.resetable").size()>0&&$("input.resetable").each(function(){resetInput($(this))}),$("#sign-in-form").size()>0&&validateSignIn($("#sign-in-form"))}function customCheckbox(){$("input.customInput").filter(":checkbox").each(function(){var n=$(this),t;n.hasClass("_processed")||(n.hide().addClass("_processed"),n.after('<span class="fake-checkbox"><\/span>')),t=n.next("span.fake-checkbox").first(),n.is(":checked")&&t.addClass("_checked");t.off("click.check").on("click.check",function(){t.hasClass("_checked")?(t.removeClass("_checked"),n.prop("checked",!1)):(t.addClass("_checked"),n.prop("checked",!0),n.hasClass("radio")&&clearOtherInputs(n))});n.off("change.check").on("change.check",function(){n.is(":checked")?(t.addClass("_checked"),n.hasClass("radio")&&clearOtherInputs(n)):t.removeClass("_checked")})})}function clearOtherInputs(n){n.parent().siblings().each(function(){var n=$(this);n.find("span.fake-checkbox").removeClass("_checked"),n.find("input.customInput.radio").prop("checked",!1)})}function resetInput(n){n.off("focus.reset").on("focus.reset",function(){n.val()==n.data("default-value")&&n.val("").addClass("_notDefault")});n.off("blur.reset").on("blur.reset",function(){n.val()==""&&n.val(n.data("default-value")).removeClass("_notDefault")})}function validateSignIn(n){n.off("submit.validate").on("submit.validate",function(t){t.preventDefault(),n.find("input.error").removeClass("error");var r=!0,i=!1;n.find("input.required").each(function(){var n=$(this);n.val()||(n.addClass("error"),i||(n.focus(),i=!0))}),r&&(validEmail($("#username").val())||(r=!1,$("#username").addClass("error"),i||($("#username").focus(),i=!0)),validPasswordLength($("#password").val())||(r=!1,$("#password").addClass("error"),i||($("#password").focus(),i=!0))),r&&n.off("submit.validate").trigger("submit")})}function validName(n){return n&&n.length>5?!0:!1}function validPhone(n){var t=/^[\d\s]{8,14}$/;return n&&t.test(n)?!0:!1}function validEmail(n){var t=/^([\w]+)(.[\w]+)*@([\w-]+)(\.[\w]{2,3}){1,2}$/;return n&&t.test(n)?!0:!1}function validPassword(n,t){return n&&t&&n.length>5&&n==t?!0:!1}function validPasswordLength(n){return n.length>5?!0:!1}function sendEmailThrougAjax(n){var t={};n.find("input").each(function(){var n=$(this),i;n.attr("name")&&(n.is(":checkbox")?n.is(":checked")&&(t[n.attr("name")]=n.val()):(i=$(this).val(),n.data("default-value")&&n.data("default-value")==n.val()&&(i=""),t[n.attr("name")]=i))}),preload($("section.drive-for-us-form")),$.ajax({url:n.attr("action"),data:t,type:"POST",cache:!1,success:function(n){var r=$(n);$("section.drive-for-us-form").html(r.html())},error:function(n,t,i){$("section.drive-for-us-form").html('<p id="error">We have encountered an error!<br />'+t+(i?" - "+i:"")+"<\/p>")}})}function preload(n){var t='<div class="preloader"><img src="img/misc/preloader2.gif" alt="loading" title="loading" /><\/div>';n.html(t)}function deviceDetection(){var n={};return n.regIOS=/(iPhone|iPod|iPad).*AppleWebKit/i,n.ua=navigator.userAgent,n.isIOS=n.regIOS.test(n.ua),n.regAndroid=/android/i,n.isAndroid=n.regAndroid.test(n.ua),n.regBlackberry=/blackberry/i,n.isBlackberry=n.regBlackberry.test(n.ua),n.regWindows=/windows phone/i,n.isWindows=n.regWindows.test(n.ua),n.isDesktop=!(n.isIOS||n.isAndroid||n.isBlackberry||n.isWindows),n}function getRealWidth(n){return n.width()+parseInt(n.css("padding-left"),10)+parseInt(n.css("padding-right"),10)}function getRealHeight(n){return n.height()+parseInt(n.css("padding-top"),10)+parseInt(n.css("padding-bottom"),10)}function validEmail(n){var t=/^([\w]+)(.[\w]+)*@([\w-]+)(\.[\w]{2,3}){1,2}$/;return n&&t.test(n)?!0:!1}function baseFont(n){n>1500?n=1500:n<1100&&(n=1100);var t=n/15;$("body").css("font-size",t+"%")}var uad=deviceDetection(),timeout={},clickTimeout={},showHeaderTimeout={};if(uad.isDesktop||$("header").addClass("_absolute"),startSwipe=!1,endSwipe=!1,newEv=!1,responsive($("#wrapper").width()),disableNativeAnchors(),$(document).ready(function(){responsive($("#wrapper").width()),$("header").width($("body").width()),appInit(),hashSlideEnable()}),$(window).resize(function(){var n,t;responsive($("#wrapper").width()),$("header").width($("body").width()),uad.isAndroid?(n=document.querySelector('meta[name="viewport"]'),n&&(n.content="width=device-width, initial-scale=1.0, maximum-scale=2.0",document.body.addEventListener("gesturestart",function(){n.content="width=device-width, initial-scale=1.0, maximum-scale=2.0"},!1))):$("header").hasClass("_signinvisible")?(t=getRealHeight($("section.signinform"))-$(window).scrollTop(),t<0&&(t=0),$("header").css("top",t+"px")):$("header").css("top","0px"),$(".dropdown-menu").each(function(){scrollableul($(this))})}),uad.isDesktop)$(window).off("scroll.winScroll").on("scroll.winScroll",function(){if($("header").hasClass("_signinvisible")){var n=getRealHeight($("section.signinform"))-$(window).scrollTop();n<0&&(n=0),$("header").css("top",n+"px")}else $("header").css("top","0px")});else{$("body").off("touchstart.winScroll").on("touchstart.winScroll",function(){$("header._absolute").size()>0&&hideHeader()});$("header").off("touchstart touchmove touchend").on("touchstart touchmove touchend",function(n){n.preventDefault(),n.stopPropagation()});$("header a").off("click.trigger touchstart.trigger").on("click.trigger touchstart.trigger",function(){$("header._absolute").size()>0&&hideHeader(),window.location.hash=$(this).attr("href"),hashSlideEnable()});windowScroll();function hideHeader(){var n=$("header._absolute");parseInt(n.css("top"),10)>getRealHeight(n)&&(n.hide(),clickTimeout=setTimeout(function(){showHeader()},1e3))}function showHeader(){var n,t;clearTimeout(clickTimeout),n=$("header._absolute"),parseInt(n.css("top"),10)!=$(window).scrollTop()&&(n.removeClass("_transition"),n.show(),t=$(window).scrollTop()-getRealHeight(n)-25,n.css("top",t+"px"),clearTimeout(showHeaderTimeout),showHeaderTimeout=setTimeout(function(){n.addClass("_transition"),n.css("top",$(window).scrollTop()+"px")},100))}function windowScroll(){$(window).off("scroll.winScroll").on("scroll.winScroll",function(){$("header._absolute").size()>0&&showHeader()})}}